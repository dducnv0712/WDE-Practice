@model IEnumerable<WAD.Models.Exam>
@{
    ViewBag.Title = "Index";
}
<section>
    <div class="container mt-5">
        <div class="row">
            <div class="col"> </div>
            <div class="col-md-9">
                <div class="card">
                    <div class="card-header d-flex justify-content-end">
                        @Html.ActionLink("Create Exam", "Create", "Home")
                    </div>
                    <div class="card-body">
                        <table class="table">
                            <thead>
                                <tr>
                                   <th scope="col">@Html.DisplayNameFor(m => m.id)</th>
                                   <th scope="col">@Html.DisplayNameFor(m => m.examSubject)</th>
                                   <th scope="col">@Html.DisplayNameFor(m => m.startTime)</th>
                                   <th scope="col">@Html.DisplayNameFor(m => m.examDate)</th>
                                   <th scope="col">@Html.DisplayNameFor(m => m.examDuration)</th>
                                   <th scope="col">@Html.DisplayNameFor(m => m.classRoom)</th>
                                   <th scope="col">@Html.DisplayNameFor(m => m.facully)</th>
                                   <th>Trạng Thái</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model)
                                {
                                    <tr>
                                        <th scope="row">@Html.DisplayFor(modelItem => item.id)</th>
                                        <th>@Html.DisplayFor(modelItem => item.examSubject)</th>
                                        <td>@Html.DisplayFor(modelItem => item.startTime)</td>
                                        <td>@Html.DisplayFor(modelItem => item.examDate)</td>
                                        <td>@Html.DisplayFor(modelItem => item.examDuration)</td>
                                        <td>@Html.DisplayFor(modelItem => item.classRoom)</td>
                                        <td>@Html.DisplayFor(modelItem => item.facully)</td>
                                        <td>
                                            done
                                        </td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
            <div class="col"> </div>
        </div>
    </div>
</section>

